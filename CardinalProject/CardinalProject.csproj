    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>4e32ed95-f8ab-42e6-bf63-7274d1349137</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BCrypt.Net-Next" Version="4.0.3" />
